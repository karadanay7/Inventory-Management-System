@using WebUI.Components.Layout.Sidebar
@using WebUI.Components.Layout.NavigationBar

@inherits LayoutComponentBase

<NetcodeHub.Packages.UI.Templates.Breeze.BreezeMainLayoutTemplate>
    <InnerTemplate>
            <AuthorizeView>
        <NetcodeHub.Packages.UI.Templates.Breeze.SidebarTemplate>
            <SidebarBrandTemplate>
                <Brand />
            </SidebarBrandTemplate>
            <SidebarContentTemplate>
                <Content />
            </SidebarContentTemplate>
        </NetcodeHub.Packages.UI.Templates.Breeze.SidebarTemplate>


        </AuthorizeView>
        <NetcodeHub.Packages.UI.Templates.Breeze.NavbarTemplate>
            <BodyTemplate>
                @Body
            </BodyTemplate>
            <NavItemEndTemplate>
                <NavItems></NavItems>
            </NavItemEndTemplate>
            <NavOnMobileScreenLogoTemplate>
                <OnMobileScreenLogo />
            </NavOnMobileScreenLogoTemplate>
    </NetcodeHub.Packages.UI.Templates.Breeze.NavbarTemplate>


    </InnerTemplate>


</NetcodeHub.Packages.UI.Templates.Breeze.BreezeMainLayoutTemplate>